package ventanas;

public class RunnableImplementator implements Runnable{
	int intArray[];
	public RunnableImplementator(int arr[]) {
		intArray=arr;
	}
	public void run()
	{
		try
		{
			new MyBarras(intArray);
		}
		catch(Exception ex)
		{
			ex.printStackTrace();
		}
	}//end run()
}//end class RunnableImplementator
